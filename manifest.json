{
  "name" : "Github Document Rendered",
  "version" : "0.1",
  "description" : "Java doc is written in html 5, rendering the UI to provide cleaner documentation",
  "permissions": [
    "activeTab",
    "*://*.github.com/*"
  ],
  "background": {
    "scripts": [
      "render.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Render",
    "default_icon": "github-doc-logo.png"
  },
  "manifest_version": 2
}
